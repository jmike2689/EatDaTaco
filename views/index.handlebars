<h1>Tacos!</h1>

<h2>Eat Da Taco!</h2>


<ul>
    {{#each burgers}}
    {{#unless sleepy}}
    {{> burgers/burger-block sleep=true}}
    {{/unless}}
    {{/each}}
</ul>
{{!-- </ul> --}}




<h2>Add a taco</h2>
<form class="create-form">

    <div class="form-group">
        <label for="ca">Taco Name:</label>
        <input type="text" id="ca" name="name">
    </div>



    <button type="submit">Add Taco</button>
</form>